# ForaProject Docker Environment Variables
# Copy this file to .env and update the values for your environment

# SQL Server Configuration
# IMPORTANT: Use a strong password in production!
SA_PASSWORD=YourStrong!Passw0rd123
MSSQL_PID=Developer

# Database Configuration
DB_SERVER=sqlserver
DB_NAME=ForaProjectDb
DB_USER=sa

# ASP.NET Core Configuration
ASPNETCORE_ENVIRONMENT=Development
ASPNETCORE_URLS=http://+:80

# API Port (host port mapping)
API_PORT=5000

# Note: docker-compose.yml will use these environment variables
# The connection string is constructed automatically from these values
